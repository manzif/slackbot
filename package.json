{
  "name": "slackbot",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Manzi Fabrice",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/app.js",
    "drop_tables": "sequelize db:migrate:undo:all",
    "create_tables": "sequelize db:migrate && sequelize db:seed:all",
    "start": "node build/server.js",
    "build": "yarn create_tables && babel src --out-dir build"
  },
  "dependencies": {
    "@slack/events-api": "^3.0.0",
    "@slack/interactive-messages": "^2.0.1",
    "@slack/rtm-api": "^6.0.0",
    "@slack/web-api": "^6.1.0",
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1",
    "slackbots": "^1.2.0"
  }
}

{
  "name": "slackbot",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Manzi Fabrice",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/app.js",
    "test": "nyc --reporter=text mocha --require @babel/register --require @babel/polyfill ./src/api/*.js --timeout 80000 --exit",
    "coverage": "test && nyc report --reporter=text-lcov | coveralls",
    "start": "node build/server.js",
    "build": "yarn create_tables && babel src --out-dir build"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@slack/events-api": "^3.0.0",
    "@slack/interactive-messages": "^2.0.1",
    "@slack/rtm-api": "^6.0.0",
    "@slack/web-api": "^6.1.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "travis": "^0.1.1",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "regenerator-runtime": "^0.13.7",
    "slack-block-builder": "^1.10.0",
    "supertest": "^6.1.3"
  }
}
